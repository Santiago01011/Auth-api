{
    "version": 2,
    "functions": {
      "api/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/register",
        "dest": "/api/register.ts"
      },
      {
        "src": "/api/login",
        "dest": "/api/login.ts"
      },
      {
        "src": "/api/verify",
        "dest": "/api/verify.ts"
      }
    ]
  }
  
  